{
    "containerDefinitions": [
        {
            "name": "image-processing-container",
            "image": "account-number.dkr.ecr.us-east-1.amazonaws.com/capstone-repo:latest",
            "cpu": 256,
            "memory": 300,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "S3_BUCKET",
                    "value": "s3-bucket-name"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/image-processing-app",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "family": "image-processing-app",
    "taskRoleArn": "arn:aws:iam::account-number:role/ecs-task-role",
    "executionRoleArn": "arn:aws:iam::account-number:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "256",
    "memory": "300"
}